using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Задача_10.Последовательность
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int numberOutput = 5;

            while (numberOutput < 100)
            {
                Console.WriteLine(numberOutput);
                numberOutput += 7;
            }
            Console.ReadKey();
        }
    }
}
