using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Задача_10.Последовательность
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int numberOutput = 5;
            int maxNumberOutput = 96;
            int iterationStep = 7;

            while (numberOutput <= maxNumberOutput)
            {
                Console.WriteLine(numberOutput);
                numberOutput += iterationStep;
            }
            Console.ReadKey();
        }
    }
}
